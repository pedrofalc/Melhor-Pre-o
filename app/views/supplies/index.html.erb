<%- model_class = Supply -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:date) %></th>
      <th><%= model_class.human_attribute_name(:gasStation) %></th>
      <th><%= model_class.human_attribute_name(:km) %></th>
      <th><%= model_class.human_attribute_name(:liter) %></th>
      <th><%= model_class.human_attribute_name(:fuel) %></th>
      <th><%= model_class.human_attribute_name(:price) %></th>
      <th><%= model_class.human_attribute_name(:owner) %></th>
      <th><%= model_class.human_attribute_name(:user_id) %></th>
      <th><%= model_class.human_attribute_name(:car_id) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @supplies.each do |supply| %>
      <tr>
        <td><%= link_to supply.id, supply_path(supply) %></td>
        <td><%= supply.date %></td>
        <td><%= supply.gasStation %></td>
        <td><%= supply.km %></td>
        <td><%= supply.liter %></td>
        <td><%= supply.fuel %></td>
        <td><%= supply.price %></td>
        <td><%= supply.owner %></td>
        <td><%= supply.user_id %></td>
        <td><%= supply.car_id %></td>
        <td><%=l supply.created_at %></td>
        <td>
          <%= link_to t('.show', :default => t("helpers.links.show")),
                      supply_path(supply), :class => 'btn btn-info btn-xs' %>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_supply_path(supply), :class => 'btn btn-warning btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      supply_path(supply),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_supply_path,
            :class => 'btn btn-primary' %>
